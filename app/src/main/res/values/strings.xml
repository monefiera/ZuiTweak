    <string name="hook_change_keyboard_hotkeys_title">Change Keyboard Hotkeys</string>
    <string name="hook_change_keyboard_hotkeys_desc">Changes the hotkey functions for fn+E(DocumentsUI), fn+S(Assistant), fn+P(PC Mode).</string>
    <string name="hook_change_notification_icon_title">Change Notification Icon</string>
    <string name="hook_change_notification_icon_desc">Changes the notification icon to a CDD-compliant icon (theme applied).</string>
    <string name="hook_change_package_installer_title">Change Package Installer</string>
    <string name="hook_change_package_installer_desc">Changes the Package Installer to AOSP style.</string>
    <string name="hook_change_permission_controller_title">Change Permission Controller</string>
    <string name="hook_change_permission_controller_desc">Changes the Permission Controller to AOSP style.</string>
    <string name="hook_disable_app_switch_force_stop_title">Disable App Force Stop</string>
    <string name="hook_disable_app_switch_force_stop_desc">Disables force stop when removing an app from the App Switcher.</string>
    <string name="hook_disable_launcher_online_search_title">Remove Launcher Online Search</string>
    <string name="hook_disable_launcher_online_search_desc">Disables the Chinese online search visible in the launcher.</string>
    <string name="hook_disable_study_launcher_title">Disable Study Launcher</string>
    <string name="hook_disable_study_launcher_desc">Disables the Study launcher feature.</string>
    <string name="hook_disable_taskbar_title">Disable Taskbar Display</string>
    <string name="hook_disable_taskbar_desc">Removes the taskbar display feature during gesture navigation. (Buttons disappear in 3-button navigation)</string>
    <string name="hook_disable_virus_scan_title">Disable Virus Scan</string>
    <string name="hook_disable_virus_scan_desc">Disables the Virus Scan feature of SafeCenter.</string>
    <string name="hook_enable_battery_overheat_notify_title">Enable Battery Power Consumption Warning</string>
    <string name="hook_enable_battery_overheat_notify_desc">Enables warnings for abnormal power consumption.</string>
    <string name="hook_enable_hidden_display_settings_title">Expand Display Settings</string>
    <string name="hook_enable_hidden_display_settings_desc">Enables hidden display settings options.</string>
    <string name="hook_enable_hotspot_title">Enable Hotspot</string>
    <string name="hook_enable_hotspot_desc">Enables the hotspot feature.</string>
    <string name="hook_enable_wifidirect_title">Enable Wifi Direct</string>
    <string name="hook_enable_wifidirect_desc">Enable the Wifi Direct feature.</string>
    <string name="hook_enable_multiple_space_title">Enable Multiple Space</string>
    <string name="hook_enable_multiple_space_desc">Enables Multiple Space on Global ROM. (Requires ZuiTweak-magisk)</string>
    <string name="hook_enable_one_vision_smart_rotation_title">Enable Smart Rotation</string>
    <string name="hook_enable_one_vision_smart_rotation_desc">Enables One Vision\'s Smart Rotation feature. (Replaces Smart Split)</string>
    <string name="hook_enable_one_vision_smart_split_title">Enable Smart Split</string>
    <string name="hook_enable_one_vision_smart_split_desc">Enables One Vision\'s Smart Split feature. (Replaces Smart Rotation)</string>
    <string name="hook_enable_pen_service_title">Enable Pen Service</string>
    <string name="hook_enable_pen_service_desc">Enables Pen Service without pairing, allowing the use of compatible pens.</string>
    <string name="hook_enable_study_launcher_title">Enable Study Launcher</string>
    <string name="hook_enable_study_launcher_desc">Enables the Study launcher feature.</string>
    <string name="hook_enable_taskbar_show_recent_apps_title">Enable Show Recent Apps on Taskbar</string>
    <string name="hook_enable_taskbar_show_recent_apps_desc">Shows recent apps in the taskbar options.</string>
    <string name="hook_enable_turn_screen_on_title">Allow Screen On</string>
    <string name="hook_enable_turn_screen_on_desc">Enables the option to allow apps to turn on the screen.</string>
    <string name="hook_enable_zui_camera_shutter_option_title">Enable Camera Silent Mode Option</string>
    <string name="hook_enable_zui_camera_shutter_option_desc">Enables the silent mode option in the camera app settings.</string>
    <string name="hook_fix_battery_icon_test_mode_title">Fix Battery Icon Disappearance</string>
    <string name="hook_fix_battery_icon_test_mode_desc">Fixes the issue where the battery icon disappears when test_mode is enabled.</string>
    <string name="hook_fix_documents_ui_crash_title">Fix DocumentsUI Crash</string>
    <string name="hook_fix_documents_ui_crash_desc">Fixes the crash issue that occurs when dragging with a mouse in DocumentsUI.</string>
    <string name="hook_disable_keyboard_hotkey_menu_title">Disable Keyboard Shortcut Hotkey</string>
    <string name="hook_disable_keyboard_hotkey_menu_desc">Removes the keyboard shortcut hotkey screen that appears when you press Ctrl for 3 seconds.</string>
    <string name="hook_remove_lockscreen_shortcuts_title">Remove Lockscreen Shortcuts</string>
    <string name="hook_remove_lockscreen_shortcuts_desc">Removes the shortcuts at the bottom of the lockscreen.</string>
    <string name="hook_remove_prc_info_title">Remove PRC Info</string>
    <string name="hook_remove_prc_info_desc">Removes hidden PRC-related information.</string>
    <string name="hook_remove_shutdown_message_title">Remove Shutdown Message</string>
    <string name="hook_remove_shutdown_message_desc">Removes the shutdown message displayed at the bottom.</string>
    <string name="hook_remove_unused_game_service_icons_title">Remove Unused Game Service Icons</string>
    <string name="hook_remove_unused_game_service_icons_desc">Removes QQ, WeChat, Line, and WhatsApp icons from the game service.</string>
